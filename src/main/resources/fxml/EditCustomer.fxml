<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox prefHeight="288.0" prefWidth="600.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox maxWidth="-Infinity" prefHeight="288.0" prefWidth="286.0">
         <children>
            <Label text="Ändra kunduppgifter">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets left="30.0" top="10.0" />
               </VBox.margin>
            </Label>
            <Label text="Förnamn">
               <VBox.margin>
                  <Insets left="30.0" top="30.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="firstName" maxWidth="-Infinity" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </TextField>
            <Label text="Efternamn">
               <VBox.margin>
                  <Insets left="30.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="lastName" maxWidth="-Infinity" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </TextField>
            <Label text="Telefon">
               <VBox.margin>
                  <Insets left="30.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="phone" maxWidth="-Infinity" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </TextField>
            <Label text="Email">
               <VBox.margin>
                  <Insets left="30.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="email" maxWidth="-Infinity" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </TextField>
            <Label fx:id="error" text="Något gick fel och uppgifterna sparades ej!" textFill="#dd0000" visible="false">
               <VBox.margin>
                  <Insets left="30.0" top="20.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <Label text="Address">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets left="30.0" top="10.0" />
               </VBox.margin>
            </Label>
            <Label text="Gatuaddress">
               <VBox.margin>
                  <Insets left="30.0" top="30.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="street" maxWidth="-Infinity" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </TextField>
            <Label text="C/o">
               <VBox.margin>
                  <Insets left="30.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="co" maxWidth="-Infinity" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </TextField>
            <Label text="Stad">
               <VBox.margin>
                  <Insets left="30.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="city" maxWidth="-Infinity" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </TextField>
            <Label text="Postnummer">
               <VBox.margin>
                  <Insets left="30.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="zip" maxWidth="-Infinity" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </TextField>
            <HBox alignment="TOP_RIGHT" maxWidth="-Infinity" prefHeight="49.0" prefWidth="232.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="saveBtn" mnemonicParsing="false" onAction="#saveEdit" text="Spara" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="30.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#cancelEdit" text="Avbryt" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</HBox>
